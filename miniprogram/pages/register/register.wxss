/* pages/register/register.wxss */
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.form {
  width: 80%;
  margin-top: 20px;
}

input {
  margin-bottom: 10px;
  width: 100%;
  padding: 10px;
}

button {
  background-color: #007bff;
  color: white;
  border: none;
  padding: 10px 20px;
  border-radius: 5px;
  cursor: pointer;
}

button:hover {
  background-color: #0056b3;
}
